/* pages/xiang/xiang.wxss */
.img{
  width:100%;
  height:300rpx;
}
.list{
  display: flex;
  width:100%;
  height:150rpx;
  align-items: center;
}
.list text{
  margin-left: 50rpx;
}
.wenzi{
  padding:20rpx;
  box-sizing: border-box;
}
.btn{
  width:100%;
  height:150rpx;
  display: flex;
  position: absolute;
  left: 0;
  bottom: 0;
}
.btn image{
    width:100rpx;
  height:100rpx;
}
.car{
    width:20%;
  height:150rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
}
.car image{
  margin-left: 20rpx;
}
button{
  width:40%;
  color: #fff;
  font-size: 50rpx;
  line-height: 150rpx;
}