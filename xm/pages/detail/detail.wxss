/* pages/detail/detail.wxss */
.list{
  width: 100%;
  height: 150rpx;
  display: flex;
}
.one{
  display: flex;
  flex-direction: column;
  width:25%;
  text-align: center;
  align-items: center;
  justify-content: space-around;
  font-size: 30rpx;
}
.one image{
  width:80rpx;
  height: 80rpx;
}
.center{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #ccc;
  margin-top: 50rpx;
}
.center view{
  margin-left: 100rpx;
}
.qing image{
  width:150rpx;
  height:150rpx;
  margin-right: 20rpx;
}
.two{
  width:100%;
  height: 200rpx;
  display: flex;
  padding:50rpx;
  box-sizing: border-box;
}
.size{
  font-size: 35rpx;
  color: #ccc;
  margin-top: 50rpx;
  display: flex;
  justify-content: space-between;
}
.img image{
  width:50rpx;
  height:40rpx;
}