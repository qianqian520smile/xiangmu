/* pages/look/look.wxss */

page {
  width: 100%;
  height: 100%;
}

.wrap {
  width: 100%;
  height: 100%;
}

.list {
  width: 100%;
  height: 400rpx;
  /* background: pink; */
  display: flex;
  flex-wrap: wrap;
}

.list image {
  width: 100rpx;
  height: 100rpx;
  margin-top: 60rpx;
}

.one {
  display: flex;
  flex-direction: column;
  width: 20%;
  font-size: 30rpx;
  text-align: center;
  align-items: center;
  justify-content: space-between;
}

.xiang {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 30rpx;
  border-bottom: 1px solid #ccc;
}

.qing image{
  width:150rpx;
  height:150rpx;
  margin-right: 20rpx;
}
.two{
  width:100%;
  height: 200rpx;
  display: flex;
  padding:50rpx;
  box-sizing: border-box;
}
.size{
  font-size: 35rpx;
  color: #ccc;
  margin-top: 50rpx;
  display: flex;
  justify-content: space-between;
}
.img image{
  width:50rpx;
  height:40rpx;
}
.active{
  color: red;
}